{
	"name": "Swagger",
	"version": "0.1.15",
	"versionDate": "2019-12-13",
	"author": "hackolade",
	"engines": {
		"hackolade": "3.0.0",
		"hackoladePlugin": "1.0.0"
	},
	"contributes": {
		"target": {
			"applicationTarget": "SWAGGER",
			"title": "Swagger",
			"versions": [
				"2.0"
			]
		},
		"features": {
			"nestedCollections": {
				"level": 1
			},
			"disableMultipleTypes": false,
			"enableReverseEngineering": {
				"jsonDocument": true,
				"jsonSchema": true,
				"ddl": {
					"model_definitions": true,
					"entities": false
				},
				"xsd": false,
				"plugin": true 
			},
			"disableDenormalization": true,
			"enableForwardEngineering": {
				"jsonDocument": true,
				"jsonSchema": true,
				"plugin": true
			},
			"disablePatternField": false,
			"disableChoices": {
				"anyOf": true,
				"oneOf": true,
				"not": true
			},
			"enableJsonType": false,
			"disableNamingConvention": {
				"entityLevel": true,
				"containerLevel": true,
				"fieldLevel": {
					"parameter": true
				}
			},
			"disableChildren": {
				"entityLevel": true
			},
			"disableDefinitions": {
				"internal": true
			},
			"disableRelationships": true,
			"disableJsonPreview": false,
			"enableErdToggle": true,
			"enableStackedNestedCollections": true
		}
	},
	"description": "Hackolade plugin for Swagger API",
	"disabled": false
}
